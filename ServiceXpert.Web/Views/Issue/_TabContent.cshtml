@model List<Issue>
@foreach (var issue in Model)
{
<tr>
    @* Issue Key *@
    <th><span>@await Html.PartialAsync(nameof(_IssueKeyAnchorTag), new _IssueKeyAnchorTag(issue.IssueKey, issue.Name))</span></th>
    @* Summary *@
    <td><span class="issue-name">@await Html.PartialAsync(nameof(_IssueKeyAnchorTag), new _IssueKeyAnchorTag(issue.IssueKey, issue.Name, false))</span></td>
    @* Assignee *@
    <td><span>Lorem Ipsum</span></td>
    @* Reporter *@
    <td><span>Lorem Ipsum</span></td>
    @* Created *@
    <td><span>@issue.CreateDateFormatted</span></td>
    @* Issue Priority *@
    <td class="issue-priority"><span class="priority-@issue.IssuePriorityId">@issue.IssuePriority!.Name</span></td>
    @* Issue Status *@
    <td class="issue-status"><span>@issue.IssueStatus!.Name</span></td>
</tr>
}